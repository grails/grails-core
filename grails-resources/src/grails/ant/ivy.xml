<?xml version="1.0" encoding="UTF-8"?>
<ivy-module version="2.0">
    <info organisation="org.example" module="@grails.project.name@"/>
    <configurations defaultconfmapping="build->default">
        <conf name="build"/>
    </configurations>
    <dependencies>
        <dependency org="org.grails" name="grails-dependencies" rev="@grails.version@" conf="build"/>
	<dependency org="org.apache.ivy" name="ivy" rev="2.2.0"/>
	<dependency org="org.codehaus.gant" name="gant_groovy1.8" rev="1.9.9" conf="build"/>
	<dependency org="jline" name="jline" rev="1.0" conf="build"/>
	<dependency org="org.fusesource.jansi" name="jansi" rev="1.10" conf="build"/>
    </dependencies>
</ivy-module>
